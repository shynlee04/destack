{
  "installCommand": "npm ci --production=false",
  "buildCommand": "cp -r dev/nextjs-project/data . && npm run build:next",
  "outputDirectory": "dev/nextjs-project/.next",
  "framework": "nextjs",
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=300, stale-while-revalidate=86400"
        }
      ]
    }
  ]
}